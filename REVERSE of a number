import java.util.*;
   
   public class Main{
       
       public static void reverse(int n)
       {
           while(n!=0)
           {
               int d= n%10;
               System.out.println(d);
               n=n/10;
           }
       }
   
   public static void main(String[] args) {
     // write your code here  
     Scanner scn=new Scanner(System.in);
int n=scn.nextInt();
reverse(n);
    }
   }
